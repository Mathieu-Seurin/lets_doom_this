{
  "algo":"PPO",

  "algo_config":{

    "num_envs_per_worker": 6,
    "num_workers" : 24,
    "num_gpus": 1,
    "gamma" : 0.99,

    "use_gae": true,
    "lambda": 0.95,

    "kl_coeff": 0.5,
    "clip_param": 0.1,

    "clip_rewards" : false,

    "collect_metrics_timeout" : 120,

    "sample_batch_size": 256,
    "train_batch_size": 256,
    "sgd_minibatch_size": 256,
    "num_sgd_iter": 10,

    "lr": 7e-4,
    "lr_schedule": null,

    "vf_share_layers": true,
    "vf_loss_coeff": 1.0,
    "entropy_coeff": 0.01,
    "vf_clip_param": 10.0,

    "kl_target": 0.01,
    "batch_mode": "complete_episodes",
    "observation_filter": "NoFilter",

    "model": {
      "use_lstm" : true,
      "max_seq_len": 5,
      "lstm_cell_size": 256,
      "lstm_use_prev_action_reward": true,

      "custom_model": "cnn_late",
      "custom_preprocessor": null,
      "custom_options" : {

        "n_layers" : 3,
        "n_channels" : [16,32,64],
        "kernel" : [[2,2],[2,2]],
        "stride" : [1,1],

        "last_layer_hidden" : 128}
    }
  }
}
