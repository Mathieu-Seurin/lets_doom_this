{
  "algo":"PPO",

  "algo_config":{

    "num_envs_per_worker": 1,
    "num_workers" : 12,
    "num_gpus": 1,
    "gamma" : 0.95,

    "use_gae": true,
    "lambda": 1.0,
    "kl_coeff": 0.2,
    "clip_param": 0.1,

    "collect_metrics_timeout" : 120,

    "sample_batch_size": 256,
    "train_batch_size": 5000,
    "sgd_minibatch_size": 256,
    "num_sgd_iter": 10,
    "lr": 5e-5,
    "lr_schedule": null,

    "vf_share_layers": true,
    "vf_loss_coeff": 1.0,
    "entropy_coeff": 0.0,
    "vf_clip_param": 10.0,
    "kl_target": 0.01,
    "batch_mode": "complete_episodes",
    "observation_filter": "MeanStdFilter",

    "model": {
      "custom_model": "base_cnn",
      "custom_preprocessor": null,
      "custom_options" : {

        "n_layers" : 2,
        "n_channels" : [8,16],
        "kernel" : [[8,8],[5,5]],
        "stride" : [4,3],

        "last_layer_hidden" : 512

    }
  }

}